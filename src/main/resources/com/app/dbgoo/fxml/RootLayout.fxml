<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.VBox?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.control.ProgressBar?>
<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.layout.StackPane?>
<VBox
        AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0"
        xmlns="http://javafx.com/javafx/8.0.121"
        fx:controller="com.app.dbgoo.RootController"
        xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <fx:include source="MenuLayout.fxml"/>
        <AnchorPane styleClass="global-bg-highlighted" cache="true" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" VBox.vgrow="ALWAYS">
            <SplitPane dividerPositions="0.3" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
                       AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                <AnchorPane prefWidth="200" maxWidth="700">
                    <StackPane AnchorPane.topAnchor="0.0"
                               AnchorPane.bottomAnchor="0.0"
                               AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0">
                    <fx:include source="LeftSideLayout.fxml" fx:id="leftSideLayout"/>
                    </StackPane>
                </AnchorPane>
                <AnchorPane>
                    <StackPane AnchorPane.topAnchor="0.0"
                               AnchorPane.bottomAnchor="0.0"
                               AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0">
                        <fx:include source="TabsLayout.fxml" fx:id="addTabsLayout"/>
                    </StackPane>
                </AnchorPane>
            </SplitPane>
        </AnchorPane>
   <!--     <VBox>
            <children>
                <ProgressBar progress="100.0"/>
            </children>
        </VBox>-->
    </children>


</VBox>